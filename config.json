{
    "languages": {
        "python": {
            "language": "python",
            "extension": "py",
            "shebang": "#!/usr/bin/env python3",
            "gitignore": "# Ignore __pycache__\n__pycache__/"
        },
        "cs_script": {
            "language": "c#",
            "extension": "csx",
            "shebang": "/usr/bin/env/ dotnet-script"
        },
        "cs_project": {
            "language": "c#",
            "extension": "cs"
        }
    },
    "ides": {
        "VS Code": {
            "name": "vscode",
            "open_command": "code %PATH%"
        }
    },
    "defaults": {
        "python": {
            "output_path": "./projects/python",
            "ide": "vscode"
        },
        "cs_script": {
            "output_path": "./projects/c#",
            "ide": "vscode"
        },
        "cs_project": {
            "output_path": "./projects/c#",
            "ide": "vscode"
        }
    }
}